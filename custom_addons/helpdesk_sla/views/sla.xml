<odoo>
    <record id="view_employee_medical_form" model="ir.ui.view">
        <field name="name">helpdesk.sla.form</field>
        <field name="model">helpdesk.sla</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='emergency']" position="after">
                <group string="Service Level Agreement" groups="hr.group_hr_user">
                    <field name="name"/>

                </group>
            </xpath>
        </field>
    </record>
</odoo>
